1
find ~ -maxdepth 1 -type f -mtime -10 -print

2
find ~ -mindepth 1 -name '*conf*' -type f -print

3
find ~ -type f -atime +20 -print

4
find /etc \( -name 'a*' -type f \) -or \( -type d -not -empty \)

5
find . -name 'x??' -type f | xargs rm
#lub
find . -name 'x??' -type f -delete

6
date +%Y-%m-%d | xargs mkdir

